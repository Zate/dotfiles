#!/usr/bin/env bash
set -e
set -o errexit
set -o nounset
set -o pipefail
export DEBIAN_FRONTEND=noninteractive
BASEDIR=$(dirname $0)
cd $BASEDIR

# First lets check some defaults
source bash_inc
echo "Init Complete, starting script"
# #are we in a container?
echo "Running check_docker"
check_docker
install_bash_inc
install_bash_rc
install_go
install_go_tools
install_aws
install_terraform


ln -s ${PWD}terraformrc ~/.terraformrc











