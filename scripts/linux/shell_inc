#     go get -u golang.org/x/tools/gopls
    [[ $D_DEBUG == true ]] && echo "end $FUNCNAME"
}

check_os_and_arch () {
    [[ $D_DEBUG == true ]] && echo "start $FUNCNAME"
    OS=$(uname -s | tr '[:upper:]' '[:lower:]')
    ARCH=$(uname -m | tr '[:upper:]' '[:lower:]')
    case "${OS}" in
        linux*)
            D_OS="linux"
            MY_OS="linux"
            ;;
        darwin*)
            D_OS="macos"
            MY_OS="macos"
            ;;
        *)
            echo "Unknown operating system"
            exit 1
            ;;
    esac
    case "${ARCH}" in
        x86_64*)
            D_ARCH="amd64"
            MY_ARCH="amd64"
            ;;
        x86)
            D_ARCH="386"
            MY_ARCH="386"
            ;;
        arm6l*)
            D_ARCH="arm6l"
            MY_ARCH="arm6l"
            ;;
        arm64*)
            D_ARCH="arm64"
            MY_ARCH="arm64"
            ;;
        aarch64*)
            D_ARCH="arm64"
            MY_ARCH="arm64"
            ;;
        *)
            echo "Unknown architecture"
            exit 1
            ;;
    esac
    OS_ARCH="${OS}-${MY_ARCH}"
    [[ $D_DEBUG == true ]] && echo "end $FUNCNAME"
    return 0
}

