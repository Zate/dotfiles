#     go get -u github.com/acroca/go-symbols
#     go get -u golang.org/x/tools/cmd/guru
#     go get -u golang.org/x/tools/cmd/gorename
#     go get -u github.com/cweill/gotests/...
#     go get -u github.com/fatih/gomodifytags
